{
	"name": "kronos/log",
	"type": "library",
	"description": "PSR-3 compliant logging component",
	"homepage": "https://github.com/kronostechnologies/kronos-log",
	"license": "MIT",
	"keywords": [
		"kronos",
		"log",
		"logging",
		"psr"
	],
	"autoload": {
		"psr-4": {
			"Kronos\\Log\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kronos\\Tests\\Log\\": "tests/"
		}
	},
	"require": {
		"php": "^5.6",
		"psr/log": "^1.0"
	},
	"suggest": {
		"guzzlehttp/guzzle": "Required for LogDNA writer",
		"sentry/sentry": "Required for Sentry writer"
	},
	"require-dev": {
		"phpunit/phpunit": "5.3.*",
		"guzzlehttp/guzzle": "^6.2",
		"sentry/sentry": "^1.7"
	}
}
