{
  "name": "kronos/log",
  "type": "library",
  "description": "PSR-3 compliant logging component",
  "homepage": "https://github.com/kronostechnologies/kronos-log",
  "license": "MIT",
  "keywords": [
    "kronos",
    "log",
    "logging",
    "psr"
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\Log\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kronos\\Tests\\Log\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "psr/log": "^3.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Required for LogDNA writer",
    "sentry/sentry": "Required for Sentry writer",
    "fluent/logger": "Required for Fluentd writer"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "7.10.0",
    "fluent/logger": "1.0.1",
    "phpunit/phpunit": "12.3.12",
    "vimeo/psalm": "6.13.1",
    "dealerdirect/phpcodesniffer-composer-installer": "1.1.2",
    "phpcompatibility/php-compatibility": "9.3.5",
    "cyclonedx/cyclonedx-php-composer": "5.2.3",
    "php-http/guzzle7-adapter": "1.1.0",
    "sentry/sentry": "4.15.2"
  },
  "config": {
    "platform": {
      "php": "8.3.17"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "cyclonedx/cyclonedx-php-composer": true,
      "php-http/discovery": true
    }
  }
}
